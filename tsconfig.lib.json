{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "dist",
    "declarationDir": "dist/types",
    "composite": false, 
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src/plugins/vue3-columns-resizable"]
}
